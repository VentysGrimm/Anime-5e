{
  "id": "anime5e",
  "title": "Anime 5E",
  "description": "A system for playing Anime 5E in Foundry VTT",
  "version": "1.0.0",
  "compatibility": {
    "minimum": "12",
    "verified": "12",
    "maximum": "13"
  },
  "authors": [
    {
      "name": "Ventys Grimm"
    }
  ],
  "esmodules": [
    "module/anime5e.js"
  ],
  "styles": [
    "styles/anime5e.css",
    "styles/character-sheet.css"
  ],
  "packs": [
    {
      "name": "attributes",
      "label": "Attributes",
      "path": "packs/attributes.db",
      "type": "Item",
      "system": "anime5e"
    },
    {
      "name": "defects",
      "label": "Defects",
      "path": "packs/defects.db",
      "type": "Item",
      "system": "anime5e"
    },
    {
      "name": "equipment",
      "label": "Equipment",
      "path": "packs/equipment.db",
      "type": "Item",
      "system": "anime5e"
    },
    {
      "name": "armor",
      "label": "Armor",
      "path": "packs/armor.db",
      "type": "Item",
      "system": "anime5e"
    },
    {
      "name": "weapons",
      "label": "Weapons",
      "path": "packs/weapons.db",
      "type": "Item",
      "system": "anime5e"
    },
    {
      "name": "vehicles",
      "label": "Vehicles",
      "path": "packs/vehicles.db",
      "type": "Item",
      "system": "anime5e"
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "grid.Distance": 5,
  "grid.Units": "ft",
  "primaryTokenAttribute": "attributes.hp",
  "secondaryTokenAttribute": "attributes.mp",
  "url": "",
  "manifest": "https://github.com/VentysGrimm/Anime-5e",
  "download": "https://github.com/VentysGrimm/Anime-5e.git",
  "license": "LICENSE.txt"
} 